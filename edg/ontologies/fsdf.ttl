# baseURI: https://linked.data.gov.au/def/fsdf
# prefix: fsdf

@prefix fsdf: <https://linked.data.gov.au/def/fsdf#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix jurisdictions: <https://linked.data.gov.au/dataset/fsdf/jurisdictions/> .
@prefix loci: <https://linked.data.gov.au/def/loci#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdo: <http://schema.org/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<https://linked.data.gov.au/def/fsdf>
  a owl:Ontology ;
  dcterms:created "2018-03-07"^^xsd:date ;
  dcterms:creator <http://orcid.org/0000-0002-8742-7730> ;
  dcterms:creator <https://linked.data.gov.au/org/ga> ;
  dcterms:modified "2021-05-14" ;
  dcterms:publisher <https://linked.data.gov.au/org/anzlic> ;
  vann:preferredNamespacePrefix "fsdf" ;
  rdfs:label "FSDF ontology" ;
  owl:versionIRI <https://linked.data.gov.au/def/fsdf/1.1> ;
  owl:versionInfo "1.0"@en ;
  owl:versionInfo "This 1.0 is an early version of the ontology but is a great departure from the 0.5 version that was published in 2018. This version concentrates on defining the FSDF concepts, not listing all of the FSDF datasets which are listed in a separate data graph within the overall FSDF Knolwedge Graph"@en ;
  skos:definition "An ontology describing the Calsses of concepts relevant to the [ANZLIC - The Spatial  Information Council](https://www.anzlic.gov.au)'s' Foundation Spatial Data Framework (FSDF) project which is \"a common reference for the assembly and maintenance of Australian and New Zealand foundation level spatial data\""@en ;
.
fsdf:Agency
  a owl:Class ;
  rdfs:subClassOf owl:Thing , prov:Agent , sdo:Organization ;
  skos:definition "An FSDF Agency is a Government Organisation that is involved with the FSDF Programme"@en ;
  skos:prefLabel "Agency"@en ;
.
fsdf:Dataset
  a owl:Class ;
  rdfs:subClassOf owl:Thing , loci:Dataset , prov:Entity ;
  skos:definition "An FSDF Dataset is a Loc-I Dataset, that is a dataset of spatial data following the Location Index (Loc-I) protocols. An FDSF Dataset has additional requirements on it for its use within the FSDF Programme, for example, the dataset must be classified according to the FSDF Themes vocabulary and must have various agents (organisations) of relevance to the dataset, such as crator and owner, recorded."@en ;
  skos:prefLabel "Dataset"@en ;
.
fsdf:Jurisdiction
  a owl:Class ;
  rdfs:subClassOf owl:Thing , prov:Agent , sdo:Organization ;
  rdfs:subClassOf dcterms:Jurisdiction ;
  rdfs:subClassOf prov:Agent ;
  owl:disjointWith fsdf:Agency ;
  skos:definition "A Jurisdiction is a Government Organization that has authority within a domain. The specific domains considered by the FSDF Programme are the Australian Commonwealth (Federal), Australian States and Territories and the Commercial sector."@en ;
  skos:prefLabel "Jurisdiction"@en ;  
.
fsdf:Mandate
  a owl:Class ;
  rdfs:subClassOf owl:Thing , dcat:Resource , prov:Entity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty fsdf:hasResponsibleAuthority ;
    ] ;
  skos:definition "An FSDF Mandate is an Australian Commonwealth or State & Territory legislation thatassigns responsibility for FSDF data or operations to an FSDF Agency. An FSDF Mandate is represented as a specialised form of DCAT Resource which ensures that a DCAT-style catalogue of instances of FSDF Mandates can be made."@en ;
  skos:prefLabel "Mandate"@en ;
.
fsdf:Theme
  a owl:Class ;
  rdfs:subClassOf owl:Thing , skos:Concept ;
  skos:definition "An FSDF Theme is a SKOS Concept, that is a taxonomy classification class of object, used by the FSDF Programme to organise FSDF Datasets"@en ;
  skos:prefLabel "Theme"@en ;
.
fsdf:affectsAgency
  a owl:ObjectProperty ;
  rdfs:domain fsdf:Mandate ;
  rdfs:range fsdf:Agency ;
  skos:definition "A Agency that is subject to the instructions of this Mandate"@en ;
  skos:prefLabel "affects Agency"@en ;
.
fsdf:hasCustodian
  a owl:ObjectProperty ;
  rdfs:domain fsdf:Dataset ;
  rdfs:range fsdf:Agency ;
  rdfs:subPropertyOf dcterms:publisher ;
  rdfs:subPropertyOf prov:agent ;
  skos:definition "The Agency that manages this Dataset"@en ;
  skos:prefLabel "has custodian"@en ;
.
fsdf:hasJurisdiction
  a owl:ObjectProperty ;
  rdfs:range fsdf:Jurisdiction ;
  rdfs:subPropertyOf dcterms:publisher ;
  rdfs:subPropertyOf prov:agent ;
  skos:definition "The Jurisdiction responsible for this object"@en ;
  skos:prefLabel "has jurisdiction"@en ;
.
fsdf:hasResponsibleAuthority
  a owl:ObjectProperty ;
  rdfs:domain fsdf:Mandate ;
  rdfs:range [
      a owl:Class ;
      owl:unionOf (
          fsdf:Agency
          fsdf:Jurisdiction
        ) ;
    ] ;
  skos:prefLabel "responsible authority"@en ;
.

fsdf:hasOperationalStatus 
  a owl:ObjectProperty ;
  rdfs:domain loci:Feature ;
  rdfs:range skos:Concept ;
  skos:definition "The mode of physical operation of the feature"@en ;
  skos:prefLabel "has operational status"@en ;
.

fsdf:featureObservation
  a owl:ObjectProperty ;
  rdfs:domain loci:Feature ;
  rdfs:range sosa:ObservableProperty ;
  skos:definition "The mode of physical operation of the feature"@en ;
  skos:prefLabel "has operational status"@en ;
.

fsdf:hasSpatialConfidence
  a owl:DatatypeProperty ;
  rdfs:domain loci:Feature ;
  rdfs:range xsd:float ;
  skos:definition "??????"@en ;
  skos:prefLabel "has spatial confidence"@en ;
.

fsdf:hasOperationalStatus
  a owl:ObjectProperty ;
  rdfs:domain loci:Feature ;
  rdfs:range skos:Concept ;
  skos:definition "??????"@en ;
  skos:prefLabel "has operational status"@en ;
.

fsdf:hasState 
  a owl:ObjectProperty ;
  rdfs:domain loci:Feature ;
  rdfs:range asgs:StateOrTerritory ;
  skos:definition "The State or Territory that this Feature is within"@en ;
  skos:prefLabel "has state"@en ;
.  
	
	